{
    "name": "davesweb/dashboard",
    "description": "Laravel admin dashboard package",
    "type": "library",
    "require": {
        "php": "^8.0",
        "illuminate/console": "^8.0",
        "illuminate/support": "^8.0",
        "laravel/fortify": "^1.8"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.2",
        "orchestra/testbench": "^6.22"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Davesweb\\Dashboard\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Davesweb\\Dashboard\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Davesweb\\Dashboard\\ServiceProvider",
                "Davesweb\\Dashboard\\RouteServiceProvider",
                "Davesweb\\Dashboard\\FortifyServiceProvider",
                "Davesweb\\Dashboard\\CrudServiceProvider"
            ]
        }
    },
    "scripts": {
        "cs-fixer": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "test": [
            "vendor/bin/phpunit tests --colors --testdox"
        ]
    }
}
